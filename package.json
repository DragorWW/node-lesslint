{
  "name": "lesslint",
  "description": "lint your less code",
  "version": "0.1.8-beta.1",
  "maintainers": [
    {
      "name": "ielgnaw",
      "email": "wuji0223@gmail.com"
    }
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "edp-core": "^1.0.28",
    "less": "^1.7.5",
    "q": "^1.1.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "fecs": "^0.4.7",
    "istanbul": "^0.3.2",
    "jasmine-node": "^1.14.5",
    "q": "^1.1.2"
  },
  "scripts": {
    "lint": "fecs lib test/**/*.spec.js --type=js",
